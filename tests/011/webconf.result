server {
    listen 9090;
    server_name localhost;
    return 301 https://$server_name:9443$request_uri;
}
server {
    listen 9443 ssl;
    server_name localhost;
    location /blog/ {
        proxy_pass http://localhost:8080/;
    }
    ssl_certificate ./011/localhost.crt;
    ssl_certificate_key ./011/localhost.key;
}
