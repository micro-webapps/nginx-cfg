server {
    listen 9090;
    server_name localhost;
    location /owncloud2/ {
        proxy_pass http://localhost:8081/;
        proxy_redirect default;
        proxy_redirect http://localhost/   /owncloud2/;
    }
    location /owncloud/ {
        proxy_pass http://localhost:8080/owncloud;
        proxy_redirect default;
        proxy_redirect http://localhost/owncloud/   /owncloud/;
    }
}
