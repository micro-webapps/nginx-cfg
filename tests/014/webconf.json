{
    "virtualhost": "localhost",
    "directories": {
        "./my-static-dir": {
            "alias": "/blog",
            "index": "index.php"
        }
    },
    "locations": {
        "/blog": {
            "proxy_protocol": "fcgi://",
            "proxy_hostname": "localhost",
            "proxy_port": "8081",
            "proxy_backend_alias": "/usr/share/wordpress/$1"
        }
    }
}
